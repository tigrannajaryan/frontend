{
  "name": "made-client",
  "version": "0.0.1",
  "author": "MadeBeauty, Inc.",
  "homepage": "http://madebeauty.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build-fast": "ionic-app-scripts build",
    "build": "ionic-app-scripts build --prod",
    "lint": "npm run lint:typescript | npm run lint:styles",
    "lint:typescript": "ionic-app-scripts lint",
    "lint:styles": "stylelint \"**/*.scss\"",
    "ionic:build": "cross-env MB_ENV=prod ionic-app-scripts build --prod",
    "ionic:serve": "cross-env MB_ENV=dev ionic-app-scripts serve",
    "ionic:serve:ios": "cross-env MB_ENV=dev ionic cordova run ios --livereload",
    "ionic:serve:android": "cross-env MB_ENV=dev ionic cordova run android --livereload",
    "mobserve": "ionic serve -c",
    "test-coverage": "ng test --preserve-symlinks --code-coverage",
    "test": "ng test --preserve-symlinks --sm=false",
    "test:watch": "echo \"Use \\033[1;33mfdescribe\\033[0m or \\033[1;33mfit\\033[0m to run only one test case\" && ng test --preserve-symlinks --watch --sm=false",
    "test-headless": "ng test --preserve-symlinks --watch=false --single-run=true --reporters=junit,progress --browsers=ChromeHeadless --sm=false",
    "e2e-test": "cd .. && make npm-test-dir-e2e BUILDDIR=client",
    "postinstall": "webdriver-manager update --gecko=false",
    "run-android": "ionic cordova run android --prod --device",
    "build-full": "npm run build && npm run test-headless && npm run e2e-test"
  },
