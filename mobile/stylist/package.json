{
  "name": "mobile",
  "version": "0.0.1",
  "author": "MadeBeauty Engineering Team",
  "homepage": "https://www.madebeauty.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "lint:styles": "stylelint \"**/*.scss\"",
    "ionic:build-prod": "cross-env MB_ENV=prod ionic-app-scripts build --prod",
    "ionic:build-staging": "cross-env MB_ENV=staging ionic-app-scripts build --prod",
    "ionic:serve": "cross-env MB_ENV=dev ionic-app-scripts serve",
    "ionic:serve-ios": "cross-env MB_ENV=dev ionic cordova emulate ios --livereload",
    "ionic:serve-android": "cross-env MB_ENV=dev ionic cordova emulate android",
    "mobserve": "ionic serve -c",
    "test-coverage": "ng test --code-coverage --preserve-symlinks",
    "test": "ng test --preserve-symlinks",
    "test-headless": "ng test --preserve-symlinks --watch=false --single-run=true --reporters=junit,progress --browsers=ChromeHeadless",
    "build-full": "npm run build && npm run test-headless"
  },
